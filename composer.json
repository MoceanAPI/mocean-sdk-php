{
  "name": "mocean/client",
  "description": "PHP Client for using Mocean's API.",
  "license": "MIT",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5.0",
    "php-http/client-implementation": "^1.0",
    "zendframework/zend-diactoros": "^1.3",
    "php-http/guzzle6-adapter": "^1.0",
    "lcobucci/jwt": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^8.0",
    "php-http/mock-client": "^1.1.0",
    "estahn/phpunit-json-assertions": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Mocean\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoceanTest\\": "test/"
    }
  }
}
