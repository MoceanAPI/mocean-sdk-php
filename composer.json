{
  "name": "mocean/client",
  "description": "PHP Client for using Mocean's API.",
  "license": "MIT",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "php-http/guzzle6-adapter": "^1.0 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "php-http/mock-client": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Mocean\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoceanTest\\": "test/"
    }
  }
}
